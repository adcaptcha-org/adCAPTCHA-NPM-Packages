{
  "compilerOptions": {
    "target": "es6",                          
    "module": "commonjs",                     
    "outDir": "./dist",                       
    "strict": true,                           
    "esModuleInterop": true,                 
    "skipLibCheck": true,
    "noImplicitAny": false,                   
    "forceConsistentCasingInFileNames": true 
  },
  "include": ["."],
  "exclude": ["dist", "node_modules"]
}